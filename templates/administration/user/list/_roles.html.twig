{% trans_default_domain 'mosparo' %}

{% if 'ROLE_ADMIN' in value %}
    <span class="text-pink">{{ 'roles.administrator'|trans }}</span>
{% elseif 'ROLE_USER' in value %}
    <span class="text-indigo">{{ 'roles.user'|trans }}</span>
{% else %}
    <span class="text-muted">{{ 'roles.inactive'|trans }}</span>
{% endif %}