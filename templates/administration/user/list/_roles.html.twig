{% trans_default_domain 'mosparo' %}

{% if 'ROLE_ADMIN' in value %}
    <span class="text-pink">{{ 'Administrator'|trans }}</span>
{% elseif 'ROLE_USER' in value %}
    <span class="text-indigo">{{ 'User'|trans }}</span>
{% else %}
    <span class="text-muted">{{ 'Inactive'|trans }}</span>
{% endif %}